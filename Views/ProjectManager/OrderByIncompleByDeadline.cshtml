@model List<TaskManagementSystem.Models.ProjectClasses.DevTask>

<h2>Order By Incomple By Deadline</h2>

@if (Model.Count > 0) {
    <h2>List Of Task</h2>
    <ol>
        @foreach (var devTask in Model) {
            <li>
                @devTask.Title
                <span> &nbsp;</span>
                @Html.ActionLink("Update", "UpdateTask", "DevTask", new { devTaskId = @devTask.Id }, null)
                <span> &nbsp;</span>
                @Html.ActionLink("Delete", "DeleteTask", "DevTask", new { devTaskId = @devTask.Id }, null)
            </li>
            <ul>
                <li>Description: @devTask.Description</li>
                <li>Priority: @devTask.Priority</li>
                <li>Status: @devTask.Status</li>
                <li>DateCreated: @devTask.DateCreated</li>
                <li>Deadline: @devTask.Deadline</li>
                <li>Developer: @devTask.Developer.UserName</li>
                <li>Percent Completed: @devTask.PercentCompleted</li>
                <li>Project Title: @devTask.Project.Title</li>
            </ul>
            <br />
            <br />
        }
    </ol>
}
@Html.ActionLink("Back to Dashboard", "Index")



